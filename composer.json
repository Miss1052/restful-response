{
    "name": "cjj/restful-response",
    "version": "1.0.0",
    "description": "Laravel RESTful API统一响应格式扩展包",
    "type": "library",
    "keywords": ["laravel", "api", "restful", "response", "json"],
    "license": "MIT",
    "authors": [
        {
            "name": "Your Name",
            "email": "your.email@example.com"
        }
    ],
    "require": {
        "php": "^8.1",
        "illuminate/support": "^10.0|^11.0",
        "illuminate/http": "^10.0|^11.0",
        "illuminate/contracts": "^10.0|^11.0",
        "illuminate/validation": "^10.0|^11.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.0",
        "orchestra/testbench": "^8.0|^9.0",
        "mockery/mockery": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "Cjj\\RestfulResponse\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Cjj\\RestfulResponse\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Cjj\\RestfulResponse\\Providers\\RestfulResponseServiceProvider"
            ],
            "aliases": {
                "ApiResponse": "Cjj\\RestfulResponse\\Facades\\ApiResponse"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
